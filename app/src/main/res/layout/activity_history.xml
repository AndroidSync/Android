<?xml version="1.0" encoding="utf-8"?>
<ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/activity_history"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="co.zync.android.activities.HistoryActivity">
    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                  android:id="@+id/history_layout" android:paddingBottom="10dp"
                  android:outlineProvider="bounds"
                  android:orientation="vertical" android:clipToPadding="false" android:background="@android:color/background_light">
    </LinearLayout>
</ScrollView>
